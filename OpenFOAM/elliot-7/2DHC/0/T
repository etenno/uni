FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293;

boundaryField
{
    topWall
    {
        type            fixedValue;
        value           uniform 393;
    }

    otherWalls
    {
        type            fixedValue;
        value           uniform 293;
    }

}

