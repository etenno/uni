        );
    }

    outlet2
    {
        type cyclic;
	neighbourPatch inlet2;
	faces
        (
            (29 31 30 28)
        );
    }

    fixedWallsTOP
    {
        type wall;
        faces
        (
	    (2 3 7 6)
	    (7 6 10 11)
	    (6 13 15 10)
	    (13 17 19 15)
